    "type": "git",
    "url": "git+https://github.com/VitorLuizC/predicates.git"
  },
  "keywords": [
    "predicate",
    "predicates",
    "composable",
    "compose",
    "curry"
  ],
  "author": {
    "url": "https://vitorluizc.github.io/",
    "name": "Vitor Luiz Cavalcanti",
    "email": "vitorluizc@outlook.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VitorLuizC/predicates/issues"
  },
  "homepage": "https://github.com/VitorLuizC/predicates#readme",
  "devDependencies": {
    "bili": "^4.2.5",
    "rollup-plugin-typescript2": "^0.19.2",
    "typescript": "^3.3.3"
  },
  "bili": {
    "input": "src/index.ts",
    "output": {
      "format": ["es", "cjs", "umd", "umd-min"],
      "moduleName": "predicates"
    },
    "plugins": {
